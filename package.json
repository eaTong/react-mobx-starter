{
  "name": "electron-mobx",
  "description": "starter for create desktop application with electron & react & Mobx ",
  "version": "1.0.0",
  "author": {
    "name": "eaTong",
    "email": "eatongchou@gmail.com"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server",
    "dist": "cross-env NODE_ENV=production webpack  --config webpack.production.config.js",
    "electron": "cross-env NODE_ENV=development electron ./app/main",
    "build-linux": "build -l",
    "build-win": "build --win",
    "build-mac": "build --mac",
    "build-all": "build -mwl"
  },
  "keywords": [
    "electron",
    "react",
    "mobx"
  ],
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.4",
    "mobx": "^3.1.5",
    "mobx-react": "^4.1.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.2.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.27.2",
    "electron": "^1.6.2",
    "electron-builder": "^15.4.3",
    "html-webpack-plugin": "^2.28.0",
    "style-loader": "^0.13.2",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  }
}
